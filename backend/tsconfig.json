{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "baseUrl": ".",
    "paths": {
      "$src": ["./src"],
      "$src/*": ["./src/*"],
      "$services": ["./src/services"],
      "$services/*": ["./src/services/*"],
      "$singletons": ["./src/singletons"],
      "$singletons/*": ["./src/singletons/*"]
    }
  },
  "include": ["src", "scripts"],
  "exclude": ["node_modules"]
}